{
  "name": "nuxt-gsuite",
  "type": "module",
  "version": "1.0.0",
  "description": "Nuxt Google Module",
  "license": "MIT",
  "repository": "moldypenguins/nuxt-gsuite",
  "author": "Craig Roberts <craig.roberts@crdevel.com> (https://crdevel.com)",
  "engines": {
    "node": ">=",
    "npm": "~1.0.20"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/moldypenguins"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release:local": "npm run lint && npm run test && npm run prepack",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.3",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.3",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.9",
    "changelogen": "^0.5.5",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "nuxt": "^3.12.3",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.24"
  }
}
