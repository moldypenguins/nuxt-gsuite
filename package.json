{
  "name": "nuxt-gsuite",
  "type": "module",
  "version": "0.1.9",
  "description": "Nuxt 3 Google Module",
  "author": "Craig Roberts <craig.roberts@crdevel.com> (https://crdevel.com)",
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/moldypenguins"
  },
  "repository": "moldypenguins/nuxt-gsuite",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.x",
    "npm": ">=10.8.x"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "release:local": "pnpm run lint && pnpm run test && pnpm run prepack",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.3",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.3",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^22.7.5",
    "changelogen": "^0.5.5",
    "eslint": "^9.6.0",
    "eslint-plugin-antfu": "^2.3.4",
    "eslint-plugin-format": "^0.1.2",
    "nuxt": "^3.12.3",
    "typescript": "^5.5.3",
    "vitest": "1.6.0",
    "vue-tsc": "^2.0.26"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "8.0.0-alpha.41",
      "@typescript-eslint/parser": "8.0.0-alpha.41",
      "@typescript-eslint/utils": "8.0.0-alpha.41",
      "are-we-there-yet": "4.0.2",
      "gauge": "5.0.2",
      "glob": "9.3.5",
      "npmlog": "7.0.1",
      "rimraf": "6.0.1"
    }
  }
}
